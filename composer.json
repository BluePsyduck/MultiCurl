{
    "name": "bluepsyduck/multicurl",
    "description": "Simple library helping with requesting multiple resources at once using multi cUrl.",
    "homepage": "https://github.com/BluePsyduck/MultiCurl",
    "type": "library",
    "keywords": ["BluePsyduck", "MultiCurl", "Curl"],
    "license": "GPL-2.0",
    "authors": [
        {
            "name": "BluePsyduck",
            "email": "bluepsyduck@gmx.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=5.3.0",
        "ext-curl": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "~6.0"
    },
    "autoload": {
        "psr-4": {
            "BluePsyduck\\MultiCurl\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "BluePsyduckTests\\MultiCurl\\": "tests/src/",
            "BluePsyduckTestAssets\\MultiCurl\\": "tests/assets/"
        }
    }
}
